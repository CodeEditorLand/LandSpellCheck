{
  "name": "Spell",
  "displayName": "[DEPRECATED] Spelling and Grammar Checker",
  "version": "0.0.1",
  "private": false,
  "description": "[DEPRECATED] Web service is no longer active - Try `Code Spellchecker`.  See README for details.",
  "homepage": "https://github.com/CodeEditorLand/Environment#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/Environment/issues",
    "email": "smcbreen@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/Environment.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Nikola R. Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
    "vscode:prepublish": "tsc -p ./"
  },
  "contributes": {
    "commands": [
      {
        "category": "Spell Checker",
        "command": "SpellProvider.changeLanguage",
        "title": "Change Language"
      }
    ],
    "configuration": {
      "properties": {
        "spell.StopAsking": {
          "default": false,
          "description": "Spell: Don't ask again for deprication warning",
          "type": "boolean"
        }
      },
      "title": "Spell options"
    },
    "keybindings": [
      {
        "command": "editor.action.quickFix",
        "key": "Alt+."
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "open": "0.0.5",
    "request": "2.9.152",
    "teacher": "0.1.0",
    "xml2js": "0.1.13"
  },
  "devDependencies": {
    "@types/mocha": "2.2.32",
    "@types/node": "6.0.40",
    "mocha": "2.3.3",
    "typescript": "2.0.3",
    "typescript-esbuild": "0.3.6",
    "vscode": "1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "icon": "images/spellIcon.svg",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  }
}
