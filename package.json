{
  "name": "Spell",
  "displayName": "[DEPRECATED] Spelling and Grammar Checker",
  "version": "0.0.1",
  "private": "false",
  "description": "[DEPRECATED] Web service is no longer active - Try `Code Spellchecker`.  See README for details.",
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues",
    "email": "smcbreen@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
    "vscode:prepublish": "tsc -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "SpellProvider.changeLanguage",
        "title": "Change Language",
        "category": "Spell Checker"
      }
    ],
    "configuration": {
      "title": "Spell options",
      "properties": {
        "spell.StopAsking": {
          "type": "boolean",
          "default": false,
          "description": "Spell: Don't ask again for deprication warning"
        }
      }
    },
    "keybindings": [
      {
        "command": "editor.action.quickFix",
        "key": "Alt+."
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "open": "9.1.0",
    "request": "2.9.152",
    "teacher": "0.1.0",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@types/mocha": "10.0.6",
    "@types/node": "20.9.4",
    "mocha": "10.2.0",
    "typescript": "5.3.2",
    "typescript-esbuild": "0.3.1",
    "vscode": "1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "icon": "images/spellIcon.svg",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  }
}
